    local hours = math.floor(elapsed / 3600)
    local mins = math.floor((elapsed % 3600) / 60)
    local secs = math.floor(elapsed % 60)
    timerLabel.Text = string.format("Ø§Ù„ÙˆÙ‚Øª: %02d:%02d:%02d", hours, mins, secs)
