timerButton.MouseButton1Click:Connect(function()

    -- Ø¨Ø¯Ø§ÙŠØ© Ø³ÙƒØ±Ø¨Øª Timer BY Fares
    -- Thanks to 3jb

    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local TweenService = game:GetService("TweenService")
    local Workspace = game:GetService("Workspace")

    local LocalPlayer = Players.LocalPlayer
    local playerGui = LocalPlayer:WaitForChild("PlayerGui")

    local TimerConfig = {
        PRIMARY_COLOR = Color3.fromRGB(50, 0, 50),
        SECONDARY_COLOR = Color3.fromRGB(200, 50, 200),
        TEXT_COLOR = Color3.fromRGB(255, 255, 0),
        FONT = Enum.Font.GothamSemibold,
        WINDOW_SIZE_NORMAL = UDim2.new(0, 300, 0, 150),
        WINDOW_SIZE_MINIMIZED = UDim2.new(0, 150, 0, 40),
        WINDOW_POSITION = UDim2.new(0.02, 0, 0.4, 0),
        CORNER_RADIUS = 8,
        ANIMATING_TEXT_COLORS = {
            Color3.fromRGB(255, 200, 0),
            Color3.fromRGB(255, 150, 50),
            Color3.fromRGB(255, 255, 100),
            Color3.fromRGB(255, 100, 50),
        },
        ANIMATING_TEXT_SPEED = 1.5,
        MINIMIZE_BUTTON_TEXT_MIN = "-",
        MINIMIZE_BUTTON_TEXT_MAX = "ã…"
    }

    -- Ø·Ù„Ø§Ø³Ù… Ø³Ø§Ù…ÙˆØ±ÙŠØ§
    local fancyStyles = { ["fullwidth"] = { A="ï¼¡",B="ï¼¢",C="ï¼£",D="ï¼¤",E="ï¼¥",F="ï¼¦",G="ï¼§",
        H="ï¼¨",I="ï¼©",J="ï¼ª",K="ï¼«",L="ï¼¬",M="ï¼­",N="ï¼®",O="ï¼¯",P="ï¼°",Q="ï¼±",R="ï¼²",S="ï¼³",
        T="ï¼´",U="ï¼µ",V="ï¼¶",W="ï¼·",X="ï¼¸",Y="ï¼¹",Z="ï¼º", a="ï½",b="ï½‚",c="ï½ƒ",d="ï½„",e="ï½…",
        f="ï½†",g="ï½‡",h="ï½ˆ",i="ï½‰",j="ï½Š",k="ï½‹",l="ï½Œ",m="ï½",n="ï½",o="ï½",p="ï½",q="ï½‘",
        r="ï½’",s="ï½“",t="ï½”",u="ï½•",v="ï½–",w="ï½—",x="ï½˜",y="ï½™",z="ï½š", ["0"]="ï¼",["1"]="ï¼‘",
        ["2"]="ï¼’",["3"]="ï¼“",["4"]="ï¼”",["5"]="ï¼•",["6"]="ï¼–",["7"]="ï¼—",["8"]="ï¼˜",["9"]="ï¼™",
        [" "]=" " } }

    local function FancyString(text, style)
        local styleTable = fancyStyles[style] or fancyStyles["fullwidth"]
        local newText = ""
        for c in tostring(text):gmatch(".") do newText = newText .. (styleTable[c] or c) end
        return newText
    end

    -- ØªØ«Ø¨ÙŠØª Ø§Ù„Ù‡ÙˆÙƒ
    local currentAnimatingColorIndex = 1
    local guiMinimized = false
    local timeActive = 0
    local realStart = tick()
    local backupTime = 0

    local function AnimateTextColor(label)
        if not label then return end
        currentAnimatingColorIndex = (currentAnimatingColorIndex % #TimerConfig.ANIMATING_TEXT_COLORS) + 1
        local targetColor = TimerConfig.ANIMATING_TEXT_COLORS[currentAnimatingColorIndex]
        local tween = TweenService:Create(label, TweenInfo.new(TimerConfig.ANIMATING_TEXT_SPEED, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut), {TextColor3 = targetColor})
        tween:Play()
        tween.Completed:Connect(function() AnimateTextColor(label) end)
    end

    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "FaresFancyTimerGUI"
    screenGui.Parent = playerGui
    screenGui.ResetOnSpawn = false

    local mainFrame = Instance.new("Frame")
    mainFrame.Size = TimerConfig.WINDOW_SIZE_NORMAL
    mainFrame.Position = TimerConfig.WINDOW_POSITION
    mainFrame.BackgroundColor3 = TimerConfig.PRIMARY_COLOR
    mainFrame.BorderSizePixel = 1
    mainFrame.BorderColor3 = Color3.fromRGB(100, 50, 100)
    mainFrame.Active = true
    mainFrame.Draggable = true
    mainFrame.Parent = screenGui

    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, TimerConfig.CORNER_RADIUS)
    corner.Parent = mainFrame
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(1, -30, 0, 30)
    titleLabel.Position = UDim2.new(0, 0, 0, 5)
    titleLabel.BackgroundTransparency = 1
    titleLabel.Font = TimerConfig.FONT
    titleLabel.Text = FancyString("TIMER BY FARES", "fullwidth")
    titleLabel.TextColor3 = TimerConfig.TEXT_COLOR
    titleLabel.TextScaled = true
    titleLabel.TextXAlignment = Enum.TextXAlignment.Center
    titleLabel.Parent = mainFrame
    AnimateTextColor(titleLabel)

    local timeLabel = Instance.new("TextLabel")
    timeLabel.Size = UDim2.new(1, -20, 0, 40)
    timeLabel.Position = UDim2.new(0, 10, 0, 50)
    timeLabel.BackgroundTransparency = 1
    timeLabel.Font = TimerConfig.FONT
    timeLabel.Text = FancyString("00:00:00", "fullwidth")
    timeLabel.TextColor3 = TimerConfig.TEXT_COLOR
    timeLabel.TextScaled = true
    timeLabel.TextXAlignment = Enum.TextXAlignment.Center
    timeLabel.Parent = mainFrame
    AnimateTextColor(timeLabel)

    local miniLabel = Instance.new("TextLabel")
    miniLabel.Size = UDim2.new(1, 0, 1, 0)
    miniLabel.BackgroundTransparency = 1
    miniLabel.Font = TimerConfig.FONT
    miniLabel.Text = FancyString("TIME IS ACTIVE", "fullwidth")
    miniLabel.TextColor3 = TimerConfig.TEXT_COLOR
    miniLabel.TextScaled = true
    miniLabel.TextXAlignment = Enum.TextXAlignment.Center
    miniLabel.Visible = false
    miniLabel.Parent = mainFrame

    local minimizeButton = Instance.new("TextButton")
    minimizeButton.Size = UDim2.new(0, 25, 0, 25)
    minimizeButton.Position = UDim2.new(1, -30, 0, 5)
    minimizeButton.BackgroundColor3 = TimerConfig.SECONDARY_COLOR
    minimizeButton.TextColor3 = TimerConfig.TEXT_COLOR
    minimizeButton.Text = TimerConfig.MINIMIZE_BUTTON_TEXT_MIN
    minimizeButton.Font = TimerConfig.FONT
    minimizeButton.Parent = mainFrame

    local minCorner = Instance.new("UICorner")
    minCorner.CornerRadius = UDim.new(0, TimerConfig.CORNER_RADIUS-2)
    minCorner.Parent = minimizeButton

    local accountButton = Instance.new("TextButton")
    accountButton.Size = UDim2.new(0, 120, 0, 25)
    accountButton.Position = UDim2.new(0, 10, 0, 95)
    accountButton.BackgroundColor3 = TimerConfig.SECONDARY_COLOR
    accountButton.TextColor3 = TimerConfig.TEXT_COLOR
    accountButton.Text = "Account Info"
    accountButton.Font = TimerConfig.FONT
    accountButton.Parent = mainFrame

    local accCorner = Instance.new("UICorner")
    accCorner.CornerRadius = UDim.new(0, TimerConfig.CORNER_RADIUS-2)
    accCorner.Parent = accountButton

    local accGui = Instance.new("Frame")
    accGui.Size = UDim2.new(0, 250, 0, 200)
    accGui.Position = UDim2.new(0.5, -125, 0.5, -100)
    accGui.BackgroundColor3 = TimerConfig.PRIMARY_COLOR
    accGui.Visible = false
    accGui.Parent = screenGui
    accGui.Active = true
    accGui.Draggable = true

    local accCornerMain = Instance.new("UICorner")
    accCornerMain.CornerRadius = UDim.new(0, TimerConfig.CORNER_RADIUS)
    accCornerMain.Parent = accGui

    local closeButton = Instance.new("TextButton")
    closeButton.Size = UDim2.new(0, 25, 0, 25)
    closeButton.Position = UDim2.new(1, -30, 0, 5)
    closeButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    closeButton.Text = "Ã—"
    closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    closeButton.Font = TimerConfig.FONT
    closeButton.Parent = accGui

    local closeCorner = Instance.new("UICorner")
    closeCorner.CornerRadius = UDim.new(0, TimerConfig.CORNER_RADIUS-2)
    closeCorner.Parent = closeButton

    local infoLabels = {}

    local function UpdateInfo()
        for _, lbl in ipairs(infoLabels) do lbl:Destroy() end
        infoLabels = {}

        local infoText = {
            "Player ID: "..tostring(LocalPlayer.UserId),
            "Account Name: "..tostring(LocalPlayer.Name),
            "Place Name: " .. game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name, 
            "Place ID: "..tostring(game.PlaceId),
            "Current Players: "..tostring(#Players:GetPlayers()),
            "Max Players: "..tostring(Players.MaxPlayers)
        }

        for i, text in ipairs(infoText) do
            local lbl = Instance.new("TextLabel")
            lbl.Size = UDim2.new(1, -20, 0, 25)
            lbl.Position = UDim2.new(0, 10, 0, 40 + (i-1)*25)
            lbl.BackgroundTransparency = 1
            lbl.Font = TimerConfig.FONT
            lbl.TextColor3 = TimerConfig.TEXT_COLOR
            lbl.TextScaled = true
            lbl.TextXAlignment = Enum.TextXAlignment.Left
            lbl.Text = FancyString(text, "fullwidth")
            lbl.Parent = accGui
            table.insert(infoLabels, lbl)
        end
    end

    local function FadeIn(frame, labels)
        frame.Visible = true
        frame.BackgroundTransparency = 1
        for _, lbl in pairs(labels) do lbl.TextTransparency = 1 end
        TweenService:Create(frame, TweenInfo.new(0.25), {BackgroundTransparency=0}):Play()
        for _, lbl in pairs(labels) do
            TweenService:Create(lbl, TweenInfo.new(0.25), {TextTransparency=0}):Play()
        end
    end

    local function FadeOut(frame, labels)
        TweenService:Create(frame, TweenInfo.new(0.25), {BackgroundTransparency=1}):Play()
        for _, lbl in pairs(labels) do
            TweenService:Create(lbl, TweenInfo.new(0.25), {TextTransparency=1}):Play()
        end
        task.delay(0.25, function() frame.Visible = false end)
    end

    accountButton.MouseButton1Click:Connect(function()
        UpdateInfo()
        FadeIn(accGui, infoLabels)
    end)

    closeButton.MouseButton1Click:Connect(function()
        FadeOut(accGui, infoLabels)
    end)

    local function ToggleMinimize()
        local duration = 0.2
        TweenService:Create(mainFrame, TweenInfo.new(duration), {BackgroundTransparency=0.8}):Play()
        TweenService:Create(titleLabel, TweenInfo.new(duration), {TextTransparency=0.8}):Play()
        TweenService:Create(timeLabel, TweenInfo.new(duration), {TextTransparency=0.8}):Play()
        TweenService:Create(accountButton, TweenInfo.new(duration), {TextTransparency=0.8}):Play()
        task.wait(duration + 0.05)
        
        guiMinimized = not guiMinimized
        if guiMinimized then
            mainFrame.Size = TimerConfig.WINDOW_SIZE_MINIMIZED
            minimizeButton.Text = TimerConfig.MINIMIZE_BUTTON_TEXT_MAX
            titleLabel.Visible = false
            timeLabel.Visible = false
            miniLabel.Visible = true
            accountButton.Visible = false
        else
            mainFrame.Size = TimerConfig.WINDOW_SIZE_NORMAL
            minimizeButton.Text = TimerConfig.MINIMIZE_BUTTON_TEXT_MIN
            titleLabel.Visible = true
            timeLabel.Visible = true
            miniLabel.Visible = false
            accountButton.Visible = true
        end

        TweenService:Create(mainFrame, TweenInfo.new(duration), {BackgroundTransparency=0}):Play()
        TweenService:Create(titleLabel, TweenInfo.new(duration), {TextTransparency=0}):Play()
        TweenService:Create(timeLabel, TweenInfo.new(duration), {TextTransparency=0}):Play()
        TweenService:Create(accountButton, TweenInfo.new(duration), {TextTransparency=0}):Play()
    end

    minimizeButton.MouseButton1Click:Connect(ToggleMinimize)

    RunService.RenderStepped:Connect(function(delta)
        local realTime = tick() - realStart
        timeActive = timeActive + delta
        backupTime = backupTime + delta

        if math.abs(timeActive - realTime) > 1 or math.abs(timeActive - backupTime) > 1 then
            timeActive = realTime
            backupTime = realTime
        end

        if not guiMinimized then
            local hours = math.floor(timeActive / 3600)
            local minutes = math.floor((timeActive % 3600) / 60)
            local seconds = math.floor(timeActive % 60)
            timeLabel.Text = FancyString(string.format("%02d:%02d:%02d", hours, minutes, seconds), "fullwidth")
        end
    end)

end) -- Ù†Ù‡Ø§ÙŠØ© Ø§Ù„Ø²Ø±
